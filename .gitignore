# Root-level ignores
.DS_Store
.env
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore dependencies for both frontend and backend
/frontend/node_modules
/backend/node_modules
/frontend/.pnp
/backend/.pnp
/frontend/.pnp.js
/backend/.pnp.js

# Ignore build and coverage directories for both frontend and backend
/frontend/build
/backend/build
/frontend/coverage
/backend/coverage

# Ignore environment files for both frontend and backend
/frontend/.env
/frontend/.env.local
/frontend/.env.development.local
/frontend/.env.test.local
/frontend/.env.production.local

/backend/.env
/backend/.env.local
/backend/.env.development.local
/backend/.env.test.local
/backend/.env.production.local
